public class factRecursion{
    public int fact(int num){
        if(num==1){
            return 1;
        }return num * fact(num-1);
    }
    public static void main(String[] args){
        factRecursion fr = new factRecursion();
        int val = fr.fact(6);
        System.out.println("factorial is :: "+val);
    }
}
